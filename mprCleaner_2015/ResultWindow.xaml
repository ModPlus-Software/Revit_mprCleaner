<mps:ModPlusWindow x:Class="mprCleaner.ResultWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:mprCleaner"
             xmlns:mps="https://modplus.org/winfx/xaml/controls"
             mc:Ignorable="d" SizeToContent="Manual"
             Height="350" Width="450">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/ModPlusStyle;component/Styles/Controls.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <TextBox Name="TbResultMessage" VerticalScrollBarVisibility="Auto"
             VerticalAlignment="Stretch"
             HorizontalScrollBarVisibility="Disabled" TextWrapping="Wrap"></TextBox>
</mps:ModPlusWindow>
